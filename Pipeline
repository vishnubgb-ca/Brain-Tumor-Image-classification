pipeline{
    agent any

    stages{
        stage("Extract Data"){
            steps{
                sh 'python3 extract_data.py'
            }
        }
        stage("Visualize Data"){
            steps{
                sh 'python3 datavisualization.py'
            }
        }
        stage("Transform Data"){
            steps{
                sh 'python3 data_transformation.py'
            }
        }
        
    }
    post{
       
        always {
            archiveArtifacts artifacts: 'model_dataset.pkl', onlyIfSuccessful: true
        }
            }
}
